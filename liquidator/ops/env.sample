# Worker container env example
NODE_ENV=production
LOG_LEVEL=info

# RPC endpoints
RPC_ARB=
RPC_OP=
RPC_BASE=
RPC_POLYGON=

# Private mempool endpoints (optional)
# Ethereum mainnet (Flashbots Protect RPC)
PRIVTX_ETH=
# L2s: set if you have private relay support; otherwise leave blank
PRIVTX_ARB=
PRIVTX_OP=
PRIVTX_BASE=
PRIVTX_POLYGON=

# Wallet private keys (hex 0x...)
WALLET_PK_ARB=
WALLET_PK_OP=
WALLET_PK_BASE=
WALLET_PK_POLYGON=

# Aave v3 PoolAddressesProvider for Base (required to enable Base)
BASE_AAVE_V3_PROVIDER=

# Subgraph endpoints (optional overrides)
AAVE_V3_SUBGRAPH_ARB=
AAVE_V3_SUBGRAPH_OP=
AAVE_V3_SUBGRAPH_BASE=
AAVE_V3_SUBGRAPH_POLYGON=

# DB / Cache
DATABASE_URL=postgres://liquidator:liquidator@db:5432/liquidator
REDIS_URL=redis://redis:6379

# Agent / LLM configuration
OPENAI_API_KEY=
OPENAI_MODEL=gpt-4o-mini
AGENT_API_PORT=8787
AGENT_STAGE_FILE=config.staged.yaml
AGENT_PREV_FILE=config.prev.yaml
AGENT_CANARY_FILE=config.canary.yaml
AGENT_REPORT_FILE=agent/latest-report.json
AGENT_PROPOSAL_DIR=agent/proposals
