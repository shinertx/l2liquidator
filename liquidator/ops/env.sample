# Worker container env example
NODE_ENV=production
LOG_LEVEL=info

# RPC endpoints
RPC_ARB=
RPC_OP=
RPC_BASE=
RPC_POLYGON=

# Private mempool endpoints (optional)
PRIVTX_ARB=
PRIVTX_OP=
PRIVTX_BASE=
PRIVTX_POLYGON=

# Wallet private keys (hex 0x...)
